{
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:vue/vue3-recommended",
    "plugin:vue-pug/vue3-recommended"
  ],
  "plugins": [
    "pug"
  ],
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "no-tabs": 0,
    "vue/max-attributes-per-line": "off",
    "vue/multi-word-component-names": "off"
  }
}