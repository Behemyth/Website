{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "editor.formatOnSave": true,
  "cSpell.words": [
    "heroicons",
    "iconify",
    "jstoxml",
    "nuxi",
    "Nuxt",
    "nuxtjs",
    "Polski",
    "Slideover",
    "TMDB"
  ],
  "eslint.useFlatConfig": true,
  "eslint.format.enable": true,
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}